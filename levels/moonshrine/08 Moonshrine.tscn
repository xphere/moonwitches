[sub_resource type="GDScript" id=14]
script/source = "extends Node

signal completed()


func execute() -> void:
	if _condition():
		_action()
	else:
		emit_signal(\"completed\")


func _condition() -> bool:
	return get_child(0).filter()


func _action() -> void:
	var action := get_child(1)
	action.connect(\"completed\", self, \"_on_action_completed\", [], CONNECT_ONESHOT)
	action.execute()


func _on_action_completed() -> void:
	emit_signal(\"completed\")
"

[sub_resource type="GDScript" id=15]
script/source = "extends Node

export var node_path : NodePath
export var parent_path : NodePath
var _node : Node
var _parent : Node


func _ready() -> void:
	yield(owner, \"ready\")
	_node = get_node(node_path)
	_parent = get_node(parent_path)


func filter() -> bool:
	return _node.get_parent() != _parent
"

